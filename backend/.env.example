# Cerebras API Configuration (Required for Ultra-Low Latency category)
CEREBRAS_API_KEY=your_cerebras_api_key_here

# ElevenLabs API Configuration (Required for Voice Agent category)
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
ELEVENLABS_VOICE_ID=your_voice_id_here

# LiquidMetal Raindrop SmartMemory Configuration (REQUIRED for hackathon)
# Get your API key from https://liquidmetal.run after registering
# This enables Raindrop SmartMemory integration for conversation context
RAINDROP_API_KEY=your_raindrop_api_key_here

# Raindrop App Configuration (optional - defaults provided)
# RAINDROP_APP_NAME=dealfu
# RAINDROP_MEMORY_NAME=conversation-memory
# RAINDROP_VERSION=1

# Vultr Managed PostgreSQL Configuration (REQUIRED for hackathon)
# Create a managed PostgreSQL database at https://my.vultr.com/databases/
# Format: postgresql+asyncpg://user:password@host:port/database
VULTR_DATABASE_URL=postgresql+asyncpg://vultradmin:your_password@your-vultr-db-host.vultrdb.com:5432/dealfu

# Production Configuration (optional)
PRODUCTION_DOMAIN=https://your-production-domain.com
